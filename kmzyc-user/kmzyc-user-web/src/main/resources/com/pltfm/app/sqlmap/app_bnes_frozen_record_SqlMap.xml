  <select id="searchPageByVo" resultMap="ibatorgenerated_BaseResultMapResultsSysUser" parameterClass="com.pltfm.app.vobject.BnesFrozenRecord">
		select FROZEN_RECORD_ID, ACCOUNT_ID, LOGIN_ID,N_customer_type_id, FROZEN_TYPE, USERNAME, REAL_NAME, ACCOUNT,
      SUE_NAME, SUE_REAL_NAME, SUE_DATE, SUE_REASON, FROZEN_REASON,THAW_REASON, FROZEN_PICTURE, FROZEN_NUMBER,
      OPERATOR,SYS_USER_NAME,OPERATOR_DATE, CREATE_DATE, CREATED, MODIFY_DATE, MODIFIED,STATUS,N_STATUS FROM (
    select  ROW_NUMBER() OVER(order by FROZEN_RECORD_ID desc) as view_row_number, tt.* 
    ,LL.N_STATUS,ll.N_customer_type_id,uu.user_name SYS_USER_NAME from BNES_FROZEN_RECORD tt,LOGIN_INFO LL ,sys_user uu
     where tt.LOGIN_ID  =LL.N_LOGIN_ID AND TT.FROZEN_TYPE=0 and tt.OPERATOR=uu.user_id
		   <isNotNull property="username">
			  <isNotEmpty prepend="and" property="username">
				   USERNAME like '%$username$%'
			  </isNotEmpty>
		  </isNotNull>
		   <isNotNull property="status">
			  <isNotEmpty prepend="and" property="status">
				  tt.STATUS = #status:Integer#
			  </isNotEmpty>
		  </isNotNull>
		   <isNotNull property="realName">
			  <isNotEmpty prepend="and" property="realName">
				   REAL_NAME like '%$realName$%'
			  </isNotEmpty>
		  </isNotNull>
		   <isNotNull property="sueName">
			  <isNotEmpty prepend="and" property="sueName">
				   SUE_NAME like '%$sueName$%'
			  </isNotEmpty>
		  </isNotNull>
		   <isNotNull property="sueRealName">
			  <isNotEmpty prepend="and" property="sueRealName">
				   SUE_REAL_NAME like '%$sueRealName$%'
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="sysUserName">
			  <isNotEmpty prepend="and" property="sysUserName">
				   uu.USER_NAME like '%$sysUserName$%'
				   and tt.STATUS = 1
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="sueDateStart">
			  <isNotEmpty prepend="and" property="sueDateStart">
				   <![CDATA[ SUE_DATE >=#sueDateStart:Date# ]]>
			  </isNotEmpty>
		  </isNotNull>
		   <isNotNull property="sueDateEnd">
			  <isNotEmpty prepend="and" property="sueDateEnd">
				 <![CDATA[ SUE_DATE < =#sueDateEnd:Date# ]]>
			  </isNotEmpty>
		  </isNotNull>		  
		   <isNotNull property="operatorDateStart">
			  <isNotEmpty prepend="and" property="operatorDateStart">
				   <![CDATA[ OPERATOR_DATE >=#operatorDateStart:Date# ]]>
				   and tt.STATUS = 1
			  </isNotEmpty>
		  </isNotNull>
		   <isNotNull property="operatorDateEnd">
			  <isNotEmpty prepend="and" property="operatorDateEnd">
				 <![CDATA[ OPERATOR_DATE < =#operatorDateEnd:Date# ]]>
				 and tt.STATUS = 1
			  </isNotEmpty>
		  </isNotNull>
		   order by OPERATOR_DATE desc
    )
    <![CDATA[ where view_row_number <= #max# and view_row_number > #skip# ]]>
	</select>
	
