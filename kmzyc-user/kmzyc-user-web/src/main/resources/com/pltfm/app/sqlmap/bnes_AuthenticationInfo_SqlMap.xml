  <!--  ****************  search page by vo  ****************  -->
  <select id="searchPageByVo" resultMap="abatorgenerated_BnesAuthenticationInfoResultOther" parameterClass="com.pltfm.app.vobject.BnesAuthenticationInfo">
		select AUTHENTICATION_ID, ACCOUNT_ID, AUTHENTICATION_MODE, CERTIFICATE_DISCRIPTION,
      EXAMINATION_VALUE, EXAMINATION_PERSON, EXAMINATION_DATE, CREATE_DATE, CREATED_ID, MODIFY_DATE,
      MODIFIE_ID,ACCOUNT_LOGIN,NAME,ACCONUT_ID,MOBILE,n_customer_type_id,CUSTOMER_NAME
    FROM (
    select  ROW_NUMBER() OVER(order by N_ACCOUNT_ID desc) as view_row_number, tt.*,aa.ACCOUNT_LOGIN,
    aa.NAME,aa.ACCONUT_ID,aa.MOBILE,bb.customer_type_id,aa.n_customer_type_id,bb.NAME CUSTOMER_NAME
    from BNES_AUTHENTICATION_INFO tt ,ACCOUNT_INFO aa,BNES_CUSTOMER_TYPE bb
    where tt.ACCOUNT_ID=aa.N_ACCOUNT_ID and bb.customer_type_id=aa.n_customer_type_id
    
		  <isNotNull property="accountLogin">
			  <isNotEmpty prepend="and" property="accountLogin">
				aa.ACCOUNT_LOGIN like '%$accountLogin$%'
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="name">
			  <isNotEmpty prepend="and" property="name">
				aa.NAME like '%$name$%'
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="acconutId">
			  <isNotEmpty prepend="and" property="acconutId">
				aa.ACCONUT_ID like '%$acconutId$%'
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="mobile">
			  <isNotEmpty prepend="and" property="mobile">
				aa.MOBILE like '%$mobile$%'
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="examinationValue">
			  <isNotEmpty prepend="and" property="examinationValue">
				tt.EXAMINATION_VALUE like '%$examinationValue$%'
			  </isNotEmpty>
		  </isNotNull>
    )
    <![CDATA[ where view_row_number <= #max# and view_row_number > #skip# ]]>
	</select>
	
