  <select id="searchPageByVo" parameterClass="com.pltfm.sys.model.SysUser" resultMap="ibatorgenerated_BaseResultMap">
		SELECT user_id, dept_id, user_cardno, user_name, user_pwd, user_real,user_alias, user_sex, user_birthday, user_phone,
		  user_mphone, user_ip, user_lasttime, user_st, user_remark, is_enable, create_date,
		  create_user, update_date, update_user
    FROM (
    select  ROW_NUMBER() OVER(order by user_id desc) as view_row_number, tt.* from sys_user tt
     <dynamic prepend="where">
		  <isNotNull property="deptId">
			  <isNotEmpty prepend="and" property="deptId">
				dept_id = #deptId:INTEGER#
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="userReal">
			  <isNotEmpty prepend="and" property="userReal">
				user_real like #userReal:VARCHAR#
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="userName">
			  <isNotEmpty prepend="and" property="userName">
				user_name like  #userName:VARCHAR# 
			  </isNotEmpty>
		  </isNotNull>
		  <isNotNull property="userSt">
			  <isNotEmpty prepend="and" property="userSt">
				user_st = #userSt:VARCHAR#
			  </isNotEmpty>
		  </isNotNull>
	  </dynamic>
    )
    WHERE view_row_number between #skip# and #max#
	</select>


