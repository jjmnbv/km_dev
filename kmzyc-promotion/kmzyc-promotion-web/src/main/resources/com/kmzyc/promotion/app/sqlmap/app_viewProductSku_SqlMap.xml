  <!-- 查询产品基本信息和库存 -->
  <select id="queryProductSkuBycodes" resultMap="productSku_BaseResultMap"  parameterClass="java.lang.String">  
    select distinct pm.procuct_name,
	                pm.product_title,
	                pm.product_no,
	                pb.brand_name,
	                sku.product_sku_id,
	                sku.product_sku_code,
	                sku.price,
	              	nvl((max(ps.stock_quality -
                    decode(sign(ps.order_quality - 0),
                           -1,
                           0,
                           ps.order_quality)) over(partition by ps.sku_attribute_id)),0) stock_quality
	  from productmain pm, product_sku sku, product_stock ps,prod_brand pb
	 where pm.product_id = sku.product_id
	   and sku.product_sku_id = ps.sku_attribute_id(+)
	   and pm.brand_id=pb.brand_id
	   and sku.product_sku_id in ($skus$) 
	   order by instr('$skus$',sku.product_sku_id)
  </select>
</sqlMap>