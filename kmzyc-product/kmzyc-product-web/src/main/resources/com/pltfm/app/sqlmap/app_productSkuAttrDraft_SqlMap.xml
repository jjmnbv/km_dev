  <select id="findSkuAttrDraftByProductId" resultMap="ibatorgenerated_BaseResultMap_View" parameterClass="java.lang.Long">
  	select t.category_attr_id,t.category_attr_value_id,t.new_attr from PRODUCT_SKU_ATTR_DRAFT t,Product_Sku_Draft psd,product_draft pd
	where pd.product_id = psd.product_id and psd.product_sku_id = t.product_sku_id
	and pd.product_id = #productId#
	group by t.category_attr_value_id,t.category_attr_id,t.new_attr order by t.CATEGORY_ATTR_ID
  </select>

  <select id="findSkuDraftInfoByProductSkuId" resultMap="ViewSkuAttr_BaseResultMap" parameterClass="java.lang.Long" >
	  select a.product_id,
	       a.product_sku_id,
	       a.product_sku_code,
	       b.category_attr_id,
	       c.category_attr_name,
	       decode(d.category_attr_value_id,null,b.category_attr_value_id,d.category_attr_value_id) category_attr_value_id,
	       decode(b.new_attr,null,d.category_attr_value,b.new_attr) category_attr_value,
	       b.status
	  from product_sku_draft         a,
	       product_sku_attr_draft    b,
	       category_attr       c,
	       category_attr_value d
	 where a.product_sku_id = b.product_sku_id
	   and b.category_attr_value_id = d.category_attr_value_id(+)
	   and c.category_attr_id = b.category_attr_id
	   and a.product_sku_id = #productSkuId#
	 order by a.product_sku_id, b.category_attr_id, d.category_attr_value_id
  </select>

