  <insert id="saveActivityInviteSupplierEntry" parameterClass="com.pltfm.app.vobject.ActivitySupplierEntry" >
    <selectKey resultClass="java.lang.Long" keyProperty="supplierEntryId" >
           select SEQ_ACTIVITY_SUPPLIER.nextval from dual
    </selectKey>
	     insert into ACTIVITY_SUPPLIER_ENTRY (SUPPLIER_ENTRY_ID, ACTIVITY_ID, SUPPLIER_ID, ENTRY_STATUS,
	      ACTIVITY_SUPPLIER_TYPE, AUDIT_STATUS, ACTIVITY_TOTAL_PRICE, CREATE_TIME, MODIFY_TIME,
	      AUDIT_TIME, ENTRY_TIME, CREATE_USER, MODIF_USER, ENTRY_USER, AUDIT_USER, REMARK)
	    select #supplierEntryId:DECIMAL#, #activityId:DECIMAL#, #supplierId:DECIMAL#,
	      #entryStatus:DECIMAL#, #activitySupplierType:DECIMAL#, #auditStatus:DECIMAL#,
	      #activityTotalPrice:DECIMAL#, sysdate, #modifyTime:TIMESTAMP#,
	      #auditTime:TIMESTAMP#, #entryTime:TIMESTAMP#, #createUser:DECIMAL#, #modifUser:DECIMAL#,
	      #entryUser:DECIMAL#, #auditUser:DECIMAL#, #remark:VARCHAR# from dual 
	      where not exists (select 1
	          from activity_supplier_entry ase
	         where ase.activity_id = #activityId:DECIMAL#
	           and ase.supplier_id =  #supplierId:DECIMAL#)
	       and not exists (select 1
            from activity_info ai
           where ai.activity_id = #activityId:DECIMAL#
             and ai.activity_start_time &lt; sysdate)
  </insert>
  
