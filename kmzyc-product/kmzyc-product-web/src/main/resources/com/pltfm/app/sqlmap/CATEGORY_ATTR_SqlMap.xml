   <!--  ****************  search page by vo  ****************  -->
  <select id="searchPageByVo" resultMap="ibatorgenerated_BaseResultMap" parameterClass="com.pltfm.app.vobject.CategoryAttr">
    select CATEGORY_ATTR_ID, CATEGORY_ID,
    CATEGORY_ATTR_NAME, INPUT_TYPE, IS_REQ, IS_NAV, STATUS,
      SORTNO, CREATE_TIME, IS_SKU, MODIF_TIME, MODIF_USER 
    FROM (
    select  ROW_NUMBER() OVER(order by CATEGORY_ATTR_ID desc) as view_row_number, CATEGORY_ATTR_ID, CATEGORY_ID, CATEGORY_ATTR_NAME, INPUT_TYPE, IS_REQ, IS_NAV, STATUS,
      SORTNO, CREATE_TIME, IS_SKU, MODIF_TIME, MODIF_USER   
    from CATEGORY_ATTR
    <dynamic prepend="where" >
      <isNotNull prepend="and" property="categoryId" >
        CATEGORY_ID = #categoryId:DECIMAL#
      </isNotNull>
      <isNotNull prepend="and" property="status" >
        STATUS = #status:DECIMAL#
      </isNotNull>
    </dynamic>
    )
    WHERE view_row_number between #skip# and #max#
	</select>


 <!-- **************** search user's roleList by vo  ****************  -->
  <select id="getCategoryAttrCount" parameterClass="com.pltfm.app.vobject.CategoryAttr" resultMap="ibatorgenerated_CountResult">
     select count(1) the_count from CATEGORY_ATTR 
    <dynamic prepend="where" >
      <isNotNull prepend="and" property="categoryId" >
        CATEGORY_ID = #categoryId:DECIMAL#
      </isNotNull>
      <isNotNull prepend="and" property="status" >
        STATUS = #status:DECIMAL#
      </isNotNull>
    </dynamic>
  </select>
  
  <select id="findRepeatAttrName" parameterClass="com.pltfm.app.vobject.CategoryAttr" resultClass="java.lang.Integer" >
  	select count(a.category_id) from category_attr a where a.category_attr_name = #categoryAttrName# and a.status = 1 and a.is_sku = 0
  	<isNotNull prepend="and" property="categoryAttrId" >
  		a.CATEGORY_ATTR_ID != #categoryAttrId#
  	</isNotNull>
  	and  a.category_id in
 	(
 		select CATEGORY_ID as categoryId from CATEGORYS where STATUS = 1  start with 
		CATEGORY_ID = #categoryId#
      	connect by  prior  parent_id = category_id  
      	union 
		select CATEGORY_ID as categoryId from CATEGORYS where  STATUS = 1  start with 
    	CATEGORY_ID = #categoryId#
      	connect by  prior  category_id =  parent_id
      )
  </select>
  
  <select id="isRelationAttr" parameterClass="java.lang.Long" resultClass="java.lang.String" >
  	select a.category_attr_name from Category_Attr a where a.category_attr_id =(
       select distinct b.relate_attr_id from product_attr b where b.relate_attr_id = #categoryAttrId# 
	) or a.category_attr_id = (
	   select distinct c.category_attr_id from product_sku_attr c where c.category_attr_id = #categoryAttrId#
	) or a.category_attr_id =(
       select distinct b.relate_attr_id from product_attr_draft b where b.relate_attr_id = #categoryAttrId# 
	) or a.category_attr_id = (
	   select distinct c.category_attr_id from product_sku_attr_draft c where c.category_attr_id = #categoryAttrId#
	)
  </select>
  
