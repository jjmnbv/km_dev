		  </isNotEmpty>
	  </isNotNull>
	  <isNotNull property="searchDate1">
		  <isNotEmpty prepend="and" property="searchDate1">
			b.send_time &gt;= #searchDate1:Date#
		  </isNotEmpty>
	  </isNotNull>
	  <isNotNull property="searchDate2">
		  <isNotEmpty prepend="and" property="searchDate2">
			b.send_time &lt;= #searchDate2:Date#
		  </isNotEmpty>
	  </isNotNull>
	  <isNotNull property="isEnable">
		  <isNotEmpty prepend="and" property="isEnable">
			b.is_enable = #isEnable:VARCHAR#
		  </isNotEmpty>
	  </isNotNull>
