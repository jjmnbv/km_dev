  <!--  ****************  search notice page by vo  ****************  -->
  <select id="searchPageByVo" parameterClass="com.pltfm.sys.model.SysNotice" resultMap="ibatorgenerated_BaseResultMap">
	WITH view_sys_notice AS (
      SELECT *, ROW_NUMBER() OVER(order by notice_id desc) as view_row_number
	  FROM sys_notice
	  <dynamic prepend="where">
		  <isNotNull property="noticeTitle">
			  <isNotEmpty prepend="and" property="noticeTitle">
