  <!--  ****************  search my notice page by vo  ****************  -->
  <select id="searchMyNoticePageByVo" parameterClass="com.pltfm.sys.model.SysNotice" resultMap="ibatorgenerated_BaseResultMap2">
	WITH view_sys_notice AS (
      SELECT a.notice_id, a.notice_title, a.notice_type, b.send_time, b.is_read, b.is_enable, ROW_NUMBER() OVER(order by b.send_time desc) as view_row_number
	  FROM sys_notice a, sys_notice_send b 
	  where a.notice_id = b.notice_id   and  a.is_enable='1'
	  and    b.user_id=#createUser:INTEGER#
	  <isNotNull property="noticeTitle">
		  <isNotEmpty prepend="and" property="noticeTitle">
			a.notice_title like #noticeTitle:VARCHAR#
		  </isNotEmpty>
	  </isNotNull>
	  <isNotNull property="noticeType">
		  <isNotEmpty prepend="and" property="noticeType">
