  <!--  ****************  get notice Count  ****************  -->
  <select id="getMyNoticeCount" parameterClass="com.pltfm.sys.model.SysNotice" resultMap="ibatorgenerated_CountResult">
	  select count(*) as the_count
	  FROM sys_notice a, sys_notice_send b 
	  where a.notice_id = b.notice_id   and  a.is_enable='1' 
	  and    b.user_id=#createUser:INTEGER#
	  <isNotNull property="noticeTitle">
		  <isNotEmpty prepend="and" property="noticeTitle">
			a.notice_title like #noticeTitle:VARCHAR#
		  </isNotEmpty>
	  </isNotNull>
	  <isNotNull property="noticeType">
		  <isNotEmpty prepend="and" property="noticeType">
			a.notice_type = #noticeType:VARCHAR#
		  </isNotEmpty>
	  </isNotNull>
	  <isNotNull property="searchDate1">
		  <isNotEmpty prepend="and" property="searchDate1">
			b.send_time &gt;= #searchDate1:Date#
		  </isNotEmpty>
	  </isNotNull>
	  <isNotNull property="searchDate2">
		  <isNotEmpty prepend="and" property="searchDate2">
			b.send_time &lt;= #searchDate2:Date#
