  <sql id="settlementRefundCriteria">
  		<isNotEmpty property="orderCode" open="(" close=")" prepend="AND">
  			HR1.order_code=#orderCode:VARCHAR#
  		</isNotEmpty>
  		<isNotEmpty property="skuNo" open="(" close=")" prepend="AND">
  			HR1.sku_no=#skuNo:VARCHAR#
  		</isNotEmpty>
  		<isNotEmpty property="productTitle" open="(" close=")" prepend="AND">
  			HR1.product_title like '%'||#productTitle:VARCHAR#||'%'
  		</isNotEmpty>
  		<isNotEmpty property="orderAlterCode" open="(" close=")" prepend="AND">
  			HR1.order_alter_code=#orderAlterCode:VARCHAR#
  		</isNotEmpty>
  		<isNotEmpty property="serviceType" open="(" close=")" prepend="AND">
  			HR1.service_type=#serviceType:DECIMAL#
  		</isNotEmpty>
  		<isNotEmpty property="startDate" open="(" close=")" prepend="AND">
  			HR1.settlement_time &gt; #startDate:VARCHAR#
  		</isNotEmpty>
  		<isNotEmpty property="endDate" open="(" close=")" prepend="AND">
  			HR1.settlement_time &lt;= #endDate:VARCHAR#
  		</isNotEmpty>
  </sql>
  
  <select id="selectRefundSum" parameterClass="com.pltfm.app.entities.SettlementRefundCriteria" resultMap="hurlRefundResult">
  	SELECT 
  	   NVL((SELECT SUM(NVL(HR1.DEAL_NUMBER,0)) FROM  SETTLEMENT_REFUND HR1 WHERE HR1.SETTLEMENT_NO = #settlementNo:VARCHAR#
  	   		<include refid="SETTLEMENT_REFUND.settlementRefundCriteria"/>
  	   ),0) SUM_NUM,
       NVL((SELECT SUM(NVL(HR1.REFUND_MONEY,0)) FROM  SETTLEMENT_REFUND HR1 WHERE HR1.SETTLEMENT_NO = #settlementNo:VARCHAR#
       		<include refid="SETTLEMENT_REFUND.settlementRefundCriteria"/>
       ),0) SUM_REFUND,
         NVL((SELECT SUM(NVL(HR1.REFUND_FARE,0)) FROM  SETTLEMENT_REFUND HR1 WHERE HR1.SETTLEMENT_NO = #settlementNo:VARCHAR#
       		<include refid="SETTLEMENT_REFUND.settlementRefundCriteria"/>
       ),0) REFUND_FARE,
       NVL((SELECT SUM(NVL(HR1.COMMISSION,0)) FROM  SETTLEMENT_REFUND HR1 WHERE HR1.SETTLEMENT_NO = #settlementNo:VARCHAR#
       		<include refid="SETTLEMENT_REFUND.settlementRefundCriteria"/>
       ),0) SUM_COMM,
       NVL((SELECT SUM(NVL(HR1.SETTLE_ACCOUNTS,0)) FROM  SETTLEMENT_REFUND HR1 WHERE HR1.SETTLEMENT_NO = #settlementNo:VARCHAR#
       		<include refid="SETTLEMENT_REFUND.settlementRefundCriteria"/>
       ),0) SUM_ACCOUNTS,
       NVL((SELECT SUM(NVL(HR1.REFUND_COMMODITY_PV_SUM,0)) FROM  SETTLEMENT_REFUND HR1 WHERE HR1.SETTLEMENT_NO = #settlementNo:VARCHAR#
       		<include refid="SETTLEMENT_REFUND.settlementRefundCriteria"/>
       ),0) SUM_REFUND_PV
     FROM DUAL
  </select>
  
